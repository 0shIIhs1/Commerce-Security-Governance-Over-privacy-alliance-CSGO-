{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "gui",
  "version": "0.1.0",
  "identifier": "com.gui.app",
  "build": {
    "beforeDevCommand": "npm run dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "CSGO GUI",
        "width": 1300,
        "height": 600
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/logo_rsq.png",
      "favicon/favicon.ico"
    ],
    "resources": [
      "scripts/**/*",
      "runtime.conf.json",
      "icons/logo_rsq.png"
    ]
  },
  "plugins": {
    "shell": {}
  }
}
